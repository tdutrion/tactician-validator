{
  "name": "league/tactician-validator",
  "description": "Adds Symfony validator support to the Tactician Command Bus",
  "authors": [
    {
        "name": "Nicolas Eeckeloo",
        "email": "neeckeloo@gmail.com"
    }
  ],
  "keywords": [
    "tactician", "middleware", "validator"
  ],
  "license": "MIT",
  "require": {
    "php":  ">=7.0",
    "league/tactician": "^1.0",
    "league/tactician-container": "^2.0",
    "symfony/validator": "^3.3"
  },
  "autoload": {
    "psr-4": {
      "League\\Tactician\\Validator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "League\\Tactician\\Validator\\Tests\\": ["tests/"]
    }
  },
  "require-dev": {
    "phpunit/phpunit": "~6.1",
    "mockery/mockery": "~0.9.9"
  }
}
